{
  "version": 2,
  "builds": [
    {
      "src": "web/api_vercel.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "web/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "web/api_vercel.py"
    },
    {
      "src": "/(.*)",
      "dest": "web/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "STREAMLIT_SERVER_PORT": "8501",
    "STREAMLIT_SERVER_ADDRESS": "0.0.0.0"
  },
  "functions": {
    "web/api_vercel.py": {
      "runtime": "python3.9"
    },
    "web/app.py": {
      "runtime": "python3.9"
    }
  },
  "installCommand": "pip install -r requirements-vercel.txt"
}